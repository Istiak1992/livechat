		"@types/lodash.isempty": "^4.4.9",
		"@types/next-auth": "3.15.0",
		"@types/node": "^20.16.5",
		"@types/react": "^18",
		"@types/react-dom": "^18",
		"@types/uuid": "10.0.0",
		"eslint": "^8",
		"eslint-config-next": "14.2.9",
		"postcss": "^8",
		"prisma": "^5.19.1",
		"tailwindcss": "^3.4.1",
		"ts-node": "^10.9.2",
